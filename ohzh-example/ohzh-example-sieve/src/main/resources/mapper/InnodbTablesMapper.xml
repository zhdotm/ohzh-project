<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.zhdotm.ohzh.example.sieve.mapper.InnodbTablesMapper">

    <resultMap id="BaseResultMap" type="io.github.zhdotm.ohzh.example.sieve.domain.InnodbTables">
        <result property="tableId" column="TABLE_ID" jdbcType="BIGINT"/>
        <result property="name" column="NAME" jdbcType="VARCHAR"/>
        <result property="flag" column="FLAG" jdbcType="INTEGER"/>
        <result property="nCols" column="N_COLS" jdbcType="INTEGER"/>
        <result property="space" column="SPACE" jdbcType="BIGINT"/>
        <result property="rowFormat" column="ROW_FORMAT" jdbcType="VARCHAR"/>
        <result property="zipPageSize" column="ZIP_PAGE_SIZE" jdbcType="INTEGER"/>
        <result property="spaceType" column="SPACE_TYPE" jdbcType="VARCHAR"/>
        <result property="instantCols" column="INSTANT_COLS" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        TABLE_ID
        ,NAME,FLAG,
        N_COLS,SPACE,ROW_FORMAT,
        ZIP_PAGE_SIZE,SPACE_TYPE,INSTANT_COLS
    </sql>
</mapper>
